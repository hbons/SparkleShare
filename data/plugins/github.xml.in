<?xml version="1.0" encoding="UTF-8"?>
<sparkleshare>
  <plugin>
    <info>
        <_name>Github</_name>
        <_description>Collaborate on projects hosted on github.com</_description>
        <icon>github.png</icon>
        <backend>Git</backend>
    </info>
    <address>
      <value>ssh://git@github.com/</value>
      <example></example>
    </address>
    <path>
      <value></value>
      <_example>/username/project</_example>
    </path>
  </plugin>
</sparkleshare>

